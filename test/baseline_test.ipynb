   "source": [
    "# Read data \ n",
    "with open ('./ data / sanshiro.txt', encoding ='sjis') as file: \ n",
    "text = file.read () \ n",
    "\ n",
    "# File formatting \ n",
    "import re \ n",
    "text = re.split ('\\-{5,}', text) [2] \ n",
    "text = re.split ('bottom:', text) [0] \ n",
    "text = text.replace ('|','') \ n ",
    "text = re.sub ('<<. +? >>','', text) \ n",
    "text = re.sub ('[#. +?]','', Text) \ n",
    "text = re.sub ('\\ n \\ n','\\ n', text) \ n",
    "text = re.sub ('\\ r','', text) \ n",
    "text = re.sub ('','', text) \ n",
    "text = re.sub ('','', text) \ n",
    "\ n",
    "print (text [: 100]) \ n",
    "\ n",
    "print (len (text))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Convert to a list like the one below \ n",
    "# [\ n",
    "# [word 1, word 2, ....] \ n",
    "# [word 1, word 2, ....] \ n",
    "# [word 1, word 2, ....] \ n",
    "#] \ n",
    "\ n",
    "# Load library \ n",
    "from janome.tokenizer import Tokenizer \ n",
    "def extract_data (text): \ n",
    "'''If you pass a sentence, it returns a list of verbs and nouns \ n",
    "Arguments: \ n",
    "Text: one sentence \ n",
    "Return value: \ n",
    "Data: List of verbs and nouns \ n",
    "Processing overview: \ n",
    "Receives one sentence and returns a list of nouns and verbs contained in it \ n",
    "'''\ n",
    "# Instanceization \ n",
    "t = Tokenizer () \ n",
    "# List of verbs and nouns \ n",
    "data = [] \ n",
    "# Morphological analysis \ n",
    "res = t.tokenize (text) \ n",
    "for i in res: \ n",
    "data.append (i.surface) \ n",
    "return data \ n",
    "\ n",
    "# Text separated by". "\ N",
    "text_list = text.split ('.') \ N",
    "\ n",
    "# Convert each sentence to a word list (it takes a few minutes) \ n",
    "word_list =" extract_data (text) for text in text_list] "
   ]
